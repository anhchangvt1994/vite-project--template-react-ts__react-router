{
	"name": "vite-project-react",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "cross-env MODE=development vite",
		"build": "tsc && vite build",
		"build:esm": "tsc && cross-env MODE=production ESM=true vite build",
		"preview": "vite preview",
		"prettier": "tsc --noEmit && prettier src/**/*.{t,j}s{,x} --no-error-on-unmatched-pattern --check",
		"lint": "tsc --noEmit && eslint src/**/*.{t,j}s{,x} --no-error-on-unmatched-pattern --ignore-pattern node_modules/",
		"lint:fix": "npm run lint -- --fix",
		"prettier:fix": "npm run prettier -- --write",
		"format": "npm run prettier:fix && npm run lint:fix",
		"prepare": "npx husky install",
		"pre-commit": "lint-staged"
	},
	"dependencies": {
		"polished": "^4.2.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.7.0",
		"styled-components": "^5.3.6"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.4.2",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
		"@rollup/plugin-alias": "^4.0.3",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.9",
		"@vitejs/plugin-react": "^3.0.0",
		"autoprefixer": "^10.4.13",
		"commitizen": "^4.3.0",
		"commitlint": "^17.4.2",
		"cross-env": "^7.0.3",
		"cz-git": "^1.4.1",
		"eslint": "^8.32.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-import-resolver-custom-alias": "^1.3.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.3",
		"quicktype-core": "^21.0.4",
		"sass": "^1.57.1",
		"tailwindcss": "^3.2.4",
		"typescript": "^4.9.3",
		"unplugin-auto-import": "^0.12.1",
		"vite": "^4.0.0",
		"vite-plugin-environment": "^1.1.3"
	},
	"eslintConfig": {
		"extends": [
			"./config/eslint.config.cjs"
		]
	},
	"prettier": "./config/prettier.config.cjs",
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "npm run prettier && npm run lint"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		],
		"rules": {
			"type-enum": [
				2,
				"always",
				[
					"feat",
					"style",
					"test",
					"fix",
					"docs",
					"release",
					"update",
					"perf",
					"chore"
				]
			]
		}
	},
	"config": {
		"commitizen": {
			"path": "cz-git",
			"czConfig": "./config/cz.config.cjs"
		}
	}
}
